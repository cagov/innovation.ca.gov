<!doctype html>
<html lang="en" xml:lang="en">
  <head>
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
    <meta content="en" http-equiv="Content-Language">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width,initial-scale=1" name="viewport">
    <meta content="State of California Office of Digital Innovation" name="author">
    <link href="https://www.google-analytics.com" rel="preconnect">
    <meta content="#000000" name="theme-color">
    <meta content="https://digital.ca.gov/{{ previewimage }}" property="og:image">
    <meta content="304" property="og:image:height">
    <meta content="608" property="og:image:width">
    <meta content="{% if headertitle %}{{ headertitle | safe }}{% else %}{{ title | safe }}{% endif %}" property="og:title">
    <meta content="{{ meta }}" property="og:description">
    <meta content="summary_large_image" name="twitter:card">
    <title>{% if headertitle %}{{ headertitle | safe }}{% else %}{{ title | safe }}{% endif %}</title>
    <meta content="california office of digital innovation (odi)" name="description">
    <link href="/css/5c904a3bad4ab89406bd.css" rel="stylesheet"></head>
  <style type="text/css">
    .news-post h1 {
      font-size: 3rem;
      padding-bottom: 1rem;
      padding-top: 2rem;
    }
    .news-post main li,
    .news-post main p {
      font-size: 20px;
    }
    .news-post main .byline {
      font-size: 16px;
      border-bottom: 1px solid #ccc;
      margin-bottom: 10px;
      padding-bottom: 10px;
    }
    figure {
      margin-bottom: 10px;
    }
    figure iframe {
      max-width: 100%;
    }
    figure img {
      border: 1px solid #ccc;
      margin-bottom: 20px;
    }
    figcaption {
      border-bottom: 1px solid #ccc;
      margin-bottom: 15px;
      padding-bottom: 10px;
    }
    .card {
      margin-bottom: 20px;
    }
    .card-set .card {
      min-width: 200px;
      float: left;
    }
    .card-set {
      grid-gap: 20px;
    }
    .clipper {
      max-height: 288px;
      overflow: hidden;
    }
    .align-center {
      margin: 0 auto;
    }
  </style>
</head>
<body class="news-post">
  <header>
    <div class="official">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <p>State of California</p>
          </div>
        </div>
      </div>
    </div>

    <nav class="navbar navbar-inverse">
      <div class="container">
        <div class="float-left navbar-header">
          <a href="/" class="navbar-brand navbar-left">CA.gov</a>
        </div>
      </div>
    </nav>
  </header>
  <main>
    <div class="container">
      <div class="row">
        <div class="col-lg-10 align-center">
          <h1>{{title | safe }}</h1>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-10 align-center">
          {% if author %}
            <p class="text-muted byline">By {{author}}<br>{{publishdate | dateformat}}</p>
          {% endif %}
          {% if lead %}<p class="lead">{{ lead | safe }}</p>{% endif %}
          {{ content | safe }}
        </div>
      </div>
    </div>
    <div class="container">
      <div class="row">
        <div class="col-lg-8">
          <hr>
          <p>Follow us on <a href="https://twitter.com/CaliforniaODI">Twitter</a> and collaborate with us at <a href="https://github.com/cagov">GitHub</a>.</p>
          <p><a href="/blog/">Back to news home</a></p>
        </div>
      </div>
    </div>
  </main>
    <footer>
      <div class="alpha-footer footer mt-0">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              <ul class="flex-fill list list-group list-group-flush list-group-horizontal-lg">
                <li class="alpha-footer list-group-item">
                  <a href="/contact-us/">Contact Us</a>
                </li>
              </ul>
              <p>Official California State Government Website</p>
            </div>
          </div>
        </div>
      </div>
    </footer>
  <script src="https://www.googletagmanager.com/gtag/js?id=UA-154336791-1" defer="defer" async></script>
  <script>
    function gtag() {
      dataLayer.push(arguments)
    }
    function ga_send_event(a, n, t) {
      ga("gtag_UA_154336791_1.send", "event", a, n, t)
    }
    window.dataLayer = window.dataLayer || [],
    gtag("js", new Date),
    gtag("config", "UA-154336791-1")
  </script>

</html>