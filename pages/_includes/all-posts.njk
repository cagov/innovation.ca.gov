<!doctype html>
<html lang="en" xml:lang="en">
  <head>
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
    <meta content="en" http-equiv="Content-Language">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width,initial-scale=1" name="viewport">
    <meta content="Alpha.CA.gov - State of California" name="author">
    <link href="https://www.google-analytics.com" rel="preconnect">
    <meta content="#000000" name="theme-color">
    <meta content="https://alpha.ca.gov/img/thumbnail.png" property="og:image">
    <meta content="630" property="og:image:height">
    <meta content="630" property="og:image:width">
    <meta content="california office of digital innovation" property="og:title">
    <meta content="california office of digital innovation (odi)" property="og:description">
    <meta content="https://alpha.ca.gov/office-of-digital-innovation/" property="og:url">
    <meta content="summary_large_image" name="twitter:card">
    <link href="https://alpha.ca.gov/office-of-digital-innovation/" rel="canonical">
    <title>California Office of Digital Innovation</title>
    <meta content="california office of digital innovation (odi)" name="description">
    <link href="/css/5c904a3bad4ab89406bd.css" rel="stylesheet"></head>
</head>
<body>
  <header>
    <div class="official">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <p>State of California</p>
          </div>
        </div>
      </div>
    </div>

    <nav class="navbar navbar-inverse">
      <div class="container">
        <div class="float-left navbar-header">
          <a href="/" class="navbar-brand navbar-left">CA.gov</a>
        </div>
      </div>
    </nav>
  </header>
  <main>
    <div class="container-fluid jumbotron">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
            <h1>News</h1>
          </div>
        </div>
      </div>
    </div>
    <div class="container">
      <div class="col-lg-12">
        <div class="card-deck card-set row">
          {%- for post in collections.mySort -%}
            <div class="card col-sm">
              <a class="clipper" href="{{post.url}}"><img src="/{% if post.data.previewimage %}{{ post.data.previewimage }}{% else %}img/design-principles.jpg{% endif %}" class="card-img-top" alt="{{ post.data.title }}"></a>
              <div class="card-body">
                <h5 class="card-title ">
                  <a href="{{ post.url }}">{{ post.data.title }}</a>
                </h5>
                <p class="card-text text-muted">{{ post.data.publishdate | dateformat }}</p>
                <p class="card-text">{{ post.data.meta | safe }}</p>
              </div>
            </div>
          {%- endfor -%}
        </div>
      </div>
    </div>
  </main>

  <!-- Global site tag (gtag.js) - Google Analytics -->
  <script async src="https://www.googletagmanager.com/gtag/js?id=UA-20973992-35"></script>
  <script>
    window.dataLayer = window.dataLayer || [];
    function gtag(){dataLayer.push(arguments);}
    gtag('js', new Date());

    gtag('config', 'UA-20973992-35');
  </script>
</body>
</html>